{
  "name": "yact-js",
  "version": "0.1.16",
  "repository": "git@github.com:hwaterke/yact-js.git",
  "author": "Harold Waterkeyn",
  "license": "MIT",
  "bin": {
    "yact": "./build/index.js"
  },
  "scripts": {
    "prepublishOnly": "yarn build",
    "prebuild": "rm -r build || true",
    "build": "babel src --copy-files --out-dir build",
    "format": "prettier --write 'src/**/*.js'",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.46",
    "chalk": "^2.4.1",
    "commander": "^2.15.1",
    "cross-spawn": "^6.0.5",
    "fs-extra": "^6.0.0",
    "lodash": "^4.17.10",
    "readline-sync": "^1.4.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.46",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-stage-3": "^7.0.0-beta.46",
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.11.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "prettier": "^1.12.1"
  },
  "lint-staged": {
    "*.js": ["prettier --write", "eslint --max-warnings 0", "git add"],
    "*.{css,scss,json,md}": ["prettier --write", "git add"]
  }
}
